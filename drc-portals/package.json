{
  "name": "drc-portals",
  "version": "0.17.7",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "generate:primary": "prisma generate --schema=prisma/schema.prisma",
    "generate:c2m2": "prisma generate --schema=prisma/c2m2/schema.prisma",
    "generate": "npm run generate:primary && npm run generate:c2m2",
    "migrate:primary": "prisma migrate deploy --schema=prisma/schema.prisma",
    "migrate": "npm run migrate:primary",
    "build": "npm run generate && next build",
    "start": "next start",
    "lint": "next lint",
    "version": "npm run build && ts-node cli/version-compose.ts && docker-compose build drc-portal-app",
    "postversion": "docker-compose push drc-portal-app",
    "worker": "NODE_OPTIONS=\"--loader ts-node/esm\" graphile-worker"
  },
