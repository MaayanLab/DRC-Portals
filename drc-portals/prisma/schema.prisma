  synonyms                               String? @default("") @db.VarChar
  file_file_analysis_typeToanalysis_type file[]  @relation("file_analysis_typeToanalysis_type")
  node                C2M2Node               @relation(fields: [id], references: [id])

  @@schema("c2m2")
}

