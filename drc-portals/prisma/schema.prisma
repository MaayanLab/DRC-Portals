  biosample_biosample_anatomyToanatomy                   biosample[]          @relation("biosample_anatomyToanatomy")
  collection_anatomy_collection_anatomy_anatomyToanatomy collection_anatomy[] @relation("collection_anatomy_anatomyToanatomy")
  node                C2M2Node               @relation(fields: [id], references: [id])

  @@schema("c2m2")
}

